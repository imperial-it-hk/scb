<resources>
    <!--parameters of the first config page ↓-->
    <string-array name="config_menu_title_init">
        <item>@string/settings_menu_communication_type</item>
        <item>@string/settings_menu_communication_parameter</item>
        <item>@string/settings_menu_otherManage</item>
    </string-array>
    <string-array name="config_menu_title_default">
        <item>@string/settings_menu_communication_type</item>
        <item>@string/settings_menu_communication_parameter</item>
        <item>@string/settings_menu_edc_parameter</item>
        <item>@string/settings_menu_issuer_parameter</item>
        <item>@string/settings_menu_acquirer_parameter</item>
        <item>@string/settings_menu_otherManage</item>
    </string-array>

    <integer-array name="config_menu_icon_init">
        <item>@drawable/system_icon</item>
        <item>@drawable/system_icon</item>
        <item>@drawable/app_other</item>
    </integer-array>
    <integer-array name="config_menu_icon_default">
        <item>@drawable/system_icon</item>
        <item>@drawable/system_icon</item>
        <item>@drawable/app_opermag</item>
        <item>@drawable/merchant_icon</item>
        <item>@drawable/merchant_icon</item>
        <item>@drawable/app_other</item>
    </integer-array>

    <!--show the gray line or not:1 to show;0 to hide-->
    <integer-array name="config_menu_show_line_init">
        <item>1</item>
        <item>1</item>
        <item>1</item>
    </integer-array>
    <!--show the gray line or not:1 to show;0 to hide-->
    <integer-array name="config_menu_show_line_default">
        <item>1</item>
        <item>0</item>
        <item>0</item>
        <item>0</item>
        <item>1</item>
        <item>1</item>
    </integer-array>

    <!--set the menu type:0 means normal type like EDC,COMM;1 means function type-->
    <!--which can jump the function(third) page directly  -->
    <integer-array name="config_menu_type_init">
        <item>1</item>
        <item>0</item>
        <item>1</item>
    </integer-array>

    <!--set the menu type:0 means normal type like EDC,COMM;1 means function type-->
    <!--which can jump the function(third) page directly  -->
    <integer-array name="config_menu_type_default">
        <item>1</item>
        <item>0</item>
        <item>0</item>
        <item>0</item>
        <item>0</item>
        <item>1</item>
    </integer-array>
    <!--parameters of the first config page ↑-->
</resources>